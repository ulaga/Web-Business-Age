<h1>Listings</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Short description</th>
    <th>User level</th>
    <th>Sponsored item1</th>
    <th>Sponsored item2</th>
    <th>Category</th>
  </tr>

<% @list.each do |l| %>
  <tr>
	<td><%= link_to l.title, "/listing/show/#{l.id}" %></td>
	<td><%= l.shortdescription%></td>
	<td><%= l.userlevel%></td>  
	<td><%= l.sponsored_item1%></td>  
	<td><%= l.sponsored_item2%></td>  
	<td><%= l.category%></td>  
    
    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New List', listing_new_path%>
